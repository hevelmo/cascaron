    <td>{{descripcion}}</td>
    <td>{{agencia.nombre}}</td>
    <td>{{categoria.nombre}}</td>
    <td>{{marca.nombre}}</td>
    <td>{{modelo.nombre}}</td>
    <td>{{anio}}</td>
    <td class='currency-h'>{{precio}}</td>
    <td>{{cilindros}} cilindros</td>
    <td>{{transmision}}</td>
