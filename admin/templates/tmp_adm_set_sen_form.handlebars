{{!--responsive--}}
<div class='row'>
    <div class='col-lg-12'>
        <section class='panel'>
            <header class='panel-heading'>

                <span class='tools pull-right'>
                    <a href='javascript:;' class='fa fa-chevron-down'></a>
                </span>
                
                <h1 class='title_set_seminuevo'>
                    Editar Seminuevo
                </h1>

            </header>
            <div class='panel-body'>
                <div class='form'>
                    <form  id='set-sen'>
                        <div class='form-inline col-sm-12 col-md-12'>
                            <div class='row'>
                                <div class='col-sm-3 col-md-3'></div>
                                <div class='col-sm-6 col-md-6'>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-description' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Descripción
                                            </label>
                                        </label>
                                        <div>
                                            <textarea class='form-control set-sen-element' id='set-sen-description' 
                                                      name='sen_description' type='text'>{{camadpa.0.descripcion}}</textarea>
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-agency' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Agencia
                                            </label>
                                        </label>
                                        <div id='div-select-set-sen-agency'>
                                            {{!--
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-agency' name='sen_agency'>
                                                <option value=''> - Seleccione una agencia - </option>
                                                {{#each camadpa}}
                                                <option value='{{id}}'>{{nombre}}</option>
                                                {{/each}}
                                            </select>
                                            --}}
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-category' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Categoría
                                            </label>
                                        </label>
                                        <div id='div-select-set-sen-category'>
                                            {{!--
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-category' name='sen_category'>
                                                <option value=''> - Seleccione una categoría - </option>
                                                {{#each camadpa}}
                                                <option value='{{id}}'>{{nombre}}</option>
                                                {{/eaach}}
                                            </select>
                                            --}}
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-brand' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Marca
                                            </label>
                                        </label>
                                        <div id='div-select-set-sen-brand'>
                                            {{!--
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-brand' name='sen_brand'>
                                                <option value=''> - Seleccione una marca - </option>
                                                {{#each camadpa}}
                                                <option value='{{id}}'>{{nombre}}</option>
                                                {{/each}}
                                            </select>
                                            --}}
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-model' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Modelo
                                            </label>
                                        </label>
                                        <div id='div-select-set-sen-model'>
                                            {{!--
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-model' name='sen_model'>
                                                <option value=''> - Seleccione una modelo - </option>
                                                {{#each camadpa}}
                                                <option value='{{id}}'>{{nombre}}</option>
                                                {{/each}}
                                            </select>
                                            --}}
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-year' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Año
                                            </label>
                                        </label>
                                        <div>
                                            <input class='set-sen-input form-control set-sen-element' id='set-sen-year' name='sen_year' type='text' value='{{camadpa.0.anio}}' />
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-price' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Precio
                                            </label>
                                        </label>
                                        <div>
                                            <input class='set-sen-input form-control set-sen-element' id='set-sen-price' name='sen_price' type='text' value='{{camadpa.0.precio}}' />
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-cylinders' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Cilindros
                                            </label>
                                        </label>
                                        <div>
                                            <input class='set-sen-input form-control set-sen-element' id='set-sen-cylinders' name='sen_cylinders' type='text' value='{{camadpa.0.cilindros}}' />
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-transmission' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Transmisión
                                            </label>
                                        </label>
                                        <div>
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-transmission' name='sen_transmission'>
                                                <option value=''> - Seleccione un tipo de transmisión - </option>
                                                <option value='AUTOMÁTICA'>AUTOMÁTICA</option>
                                                <option value='MANUAL'>MANUAL</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-color' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Color
                                            </label>
                                        </label>
                                        <div>
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-color' name='sen_color'>
                                                <option value=''> - Seleccione un color - </option>
                                                <option value='ALPINE'>ALPINE</option>
                                                <option value='ALUMINIO'>ALUMINIO</option>
                                                <option value='AMARILLO'>AMARILLO</option>
                                                <option value='ARENA'>ARENA</option>
                                                <option value='AZUL'>AZUL</option>
                                                <option value='BLACK O'>BLACK O</option>
                                                <option value='BLANCO'>BLANCO</option>
                                                <option value='CAFE'>CAFE</option>
                                                <option value='CEREZA'>CEREZA</option>
                                                <option value='CHAMPAGNE'>CHAMPAGNE</option>
                                                <option value='GRIS'>GRIS</option>
                                                <option value='KHAKI'>KHAKI</option>
                                                <option value='NEGRO'>NEGRO</option>
                                                <option value='PLATA'>PLATA</option>
                                                <option value='ROJO'>ROJO</option>
                                                <option value='TINTO'>TINTO</option>
                                                <option value='VERDE'>VERDE</option>
                                            </select>
                                        </div>
                                    </div>

                                    <div class='form-group col-sm-12 col-md-12' style='margin-top: 10px;'>
                                        <label for='set-sen-interior' class='control-label'>
                                            <sup>
                                                <i class='fa fa-asterisk' style='position: relative; top: 4px; padding: 5px;'></i>
                                            </sup>
                                            <label>
                                                Interior
                                            </label>
                                        </label>
                                        <div>
                                            <select class='set-sen-select form-control set-sen-element m-bot15' id='set-sen-interior' name='sen_interior'>
                                                <option value=''> - Seleccione un tipo de interior - </option>
                                                <option value='TELA'>TELA</option>
                                                <option value='PIEL'>PIEL</option>
                                            </select>
                                        </div>
                                    </div>

                                </div>
                                <div class='col-sm-3 col-md-3'></div>
                                <div class='clearfix'></div>
                            </div>
                        </div>
                    </form>
                    <br>
                    <div class='form-inline col-sm-12 col-md-12'>

                        <div class='col-sm-1 col-md-2'></div>

                        <button class='btn btn-success button-resp col-sm-2 col-md-2' id='set-sen-save' data-sen-id='{{camadpa.0.id}}'>
                            Guardar
                        </button>
                        <div class='col-sm-1 col-md-1'></div>

                        <button class='btn btn-default button-resp col-sm-2 col-md-2' id='set-sen-clean' data-sen-id='{{camadpa.0.id}}'>
                            Limpiar
                        </button>
                        <div class='col-sm-1 col-md-1'></div>

                        <button class='btn btn-danger button-resp col-sm-2 col-md-2' id='set-sen-restore' data-sen-id='{{camadpa.0.id}}'>
                            Restaurar
                        </button>
                        <div class='col-sm-1 col-md-1'></div>

                    </div>
                    <div class='clearfix'></div>
                </div>
            </div>
        </section>

        <section class='panel'>
            <div class='panel-body'>
                <div class='form'>
                    <div class='form-group'>
                        <div class='col-lg-offset-3 col-lg-6' align='center'>
                            <button class='btn btn-success button-resp' id='set-sen-seminuevos-list'>
                                Listado de seminuevos
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </section>

    </div>
</div>
